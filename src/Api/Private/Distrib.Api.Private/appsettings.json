{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },"ReverseProxy": {
  "Clusters": {
    "EmailCluster": {
      "Destinations": {
        "EmailCluster/destination1": {
          "Address": "http://localhost:5216/"
        }
      }
    },
    "SubscriptionCluster": {
      "Destinations": {
        "SubscriptionCluster/destination1": {
          "Address": "http://localhost:5010/"
        }
      }
    }
  },
  "Routes": {
    "EmailRoute": {
      "ClusterId": "EmailCluster",
      "Match": {
        "Path": "email-ms/{**catch-all}"
      },
      "Transforms": [
        {
          "PathPattern": "{**catch-all}"
        }
      ]
    },
    "SubscriptionRoute": {
      "ClusterId": "SubscriptionCluster",
      "Match": {
        "Path": "subscription-ms/{**catch-all}"
      },
      "Transforms": [
        {
          "PathPattern": "{**catch-all}"
        }
      ]
    }
  }
},
  "AllowedHosts": "*"
}
